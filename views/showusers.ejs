<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List of users</title>
    <style>
        body{
            text-align: center;
        }
        table,th,td {
            margin: auto;
            border: 0.5px solid black;
        }
    </style>
</head>

<body>
    <table>
        <tr>
            <th>username</th>
            <th>age</th>
            <th>id</th>
            <th>gmail</th>
            <th>followers</th>
            <th>following</th>
        </tr>
       <% for(let user of users){ %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.age  %></td>
            <td><%= user.id  %></td>
            <td><%= user.gmail  %></td>
            <td><%= user.followers  %></td>
            <td><%= user.country  %></td>
            <td><form method="get" action="/users/<%= user.id %>/edit">
                <button>edit</button>
            </form></td>
        </tr>
       <% } %>
    </table>
</body>

</html>